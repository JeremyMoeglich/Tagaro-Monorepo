{
	"name": "tagaro-monorepo",
	"version": "0.1.0",
	"author": "JeremyMoeglich",
	"scripts": {
		"build:static": "turbo run build:static",
		"build:netlify": "turbo run build:netlify",
		"build:vercel": "turbo run build:vercel",
		"build": "turbo run build",
		"dev": "dotenv -- turbo run dev --concurrency 16",
		"preview": "turbo run preview",
		"check": "turbo run check",
		"lint": "turbo run lint",
		"start": "dotenv -- turbo run start",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"ts-check": "tsc --noEmit",
		"generate": "dotenv -- turbo run generate"
	},
	"devDependencies": {
		"dotenv-cli": "^7.3.0",
		"prettier": "~3.1.0",
		"turbo": "1.10.16",
		"typescript": "^5.3.2"
	}
}
