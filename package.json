{
	"name": "tagaro-web",
	"version": "0.1.0",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"build:static": "cross-env ADAPTER=static npm run build",
		"build:node": "cross-env ADAPTER=node npm run build",
		"build:netlify": "cross-env ADAPTER=netlify npm run build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'JSDoc:ignore'",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@macfja/svelte-multi-adapter": "^1.0.2",
		"@sveltejs/adapter-netlify": "^1.0.0-next.41",
		"@sveltejs/adapter-node": "^1.0.0-next.64",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/kit": "next",
		"@types/js-cookie": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"autoprefixer": "^10.4.2",
		"babel-cli": "^6.26.0",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"emailjs": "^3.7.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"object-sizeof": "^1.6.1",
		"postcss": "^8.4.5",
		"postcss-html": "^1.3.0",
		"postcss-nested": "^5.0.6",
		"postcss-scss": "^4.0.3",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.6.0",
		"redis": "^3.1.2",
		"sass": "^1.49.0",
		"svelte": "^3.46.2",
		"svelte-check": "^2.3.0",
		"svelte-image": "^0.2.9",
		"svelte-preprocess": "^4.10.2",
		"svelte-preprocess-import-assets": "^0.1.5",
		"svelte-sequential-preprocessor": "^0.0.7",
		"svelte-use-click-outside": "^1.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4",
		"vite": "^2.7.13",
		"vite-imagetools": "^3.9.0",
		"@types/lodash.clonedeep": "^4.5.6",
		"@babel/core": "^7.16.10",
		"@types/babel-core": "^6.25.7"
	},
	"type": "module",
	"dependencies": {
		"js-cookie": "^3.0.1",
		"lodash.clonedeep": "^4.5.0"
	}
